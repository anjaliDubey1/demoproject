<h1>Employee dashbord</h1>
<table>
    <tr>
        <th>name</th>
    </tr>
    <% @employees.each do |employee| %>
    <tr>
        <td><%=employee.name %></td>
        <td><%=link_to "Profile",employee_path(employee.id) %></td>
        <td><%=link_to "Edit",edit_employee_path(employee.id) %></td>
        <td><%=link_to "Delete",employee_path(employee.id), method: :delete%></td>
    </tr>
    <% end %>
</table><br>
<%= link_to "Add Employee",new_employee_path %><br>
<%= link_to "Admin Dashboard",admins_path %><br>
<%= link_to "hr dashboard",hr_index_path %><br>